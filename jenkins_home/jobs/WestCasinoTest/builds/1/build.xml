    <script>def name = env.NAME
def remote = [:]
remote.name = &quot;westuk&quot;
remote.allowAnyHosts = true
remote.port = Integer.parseInt(env.SSH_PORT)
remote.host = env.SSH_HOST

pipeline {
  agent {
    docker {
      image &apos;node:8-alpine&apos;
    }
  }

  stages {
    stage(&apos;Build&apos;) {
      steps {
        slackSend message: &quot;${JOB_NAME}(#${BUILD_NUMBER}): build started.&quot;

        sh &apos;yarn install&apos;
        sh &apos;yarn build&apos;
        script {
          try {
            // different timeout syntax on busybox
            // build-sitemap not working sometimes
            sh &apos;timeout -t 20 yarn run build-sitemap&apos;
          } catch (e) {
            echo e.toString()
          }
        }

        sh &apos;tar czf code.tar.gz ./build&apos;
      }
    }

    stage(&apos;Deploy&apos;) {
      steps {
        withCredentials([sshUserPrivateKey(credentialsId: &apos;sshDeploy&apos;, keyFileVariable: &apos;identity&apos;, passphraseVariable: &apos;&apos;, usernameVariable: &apos;userName&apos;)]) {
          script {
            remote.user = userName
            remote.identityFile = identity
          }

          sshPut remote: remote, from: &apos;code.tar.gz&apos;, into: &apos;/opt/west-casino/frontend&apos;
          sshCommand remote: remote, command: &apos;&apos;&apos;
            cd /opt/west-casino/frontend
            tar xzvf code.tar.gz
            rm code.tar.gz

            systemctl reload nginx
          &apos;&apos;&apos;
        }
      }
    }
  }

  post {
    success {
      slackSend color: &apos;good&apos;, message: &quot;${JOB_NAME}(#${BUILD_NUMBER}): built successfully.&quot;
    }
    aborted {
      slackSend color: &apos;warning&apos;, message: &quot;${JOB_NAME}(#${BUILD_NUMBER}): build canceled.&quot;
    }
    failure {
      slackSend color: &apos;danger&apos;, message: &quot;${JOB_NAME}(#${BUILD_NUMBER}): build failed!&quot;
    }
  }
}
</script>
